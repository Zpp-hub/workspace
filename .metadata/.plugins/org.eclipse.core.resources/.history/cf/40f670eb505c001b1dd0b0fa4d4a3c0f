package com.ali.test;

class Rectangle {
	// 属性
	double length;
	double width;

}

class CalRectangle{
	// 类
	public static void main(String[] args) {
		// 创建对象
		Rectangle r1 = new Rectangle();
		Rectangle r2 = new Rectangle();
		
		// 赋值
		r1.length = 3;
		r1.width = 2;
		
		r2.length = 4;
		r2.width = 8;
		// 求面积
		System.out.println("圆形r1的面积是：" + r1.length*r1.width);
		System.out.println("圆形r1的面积是：" + r2.length*r2.width);
	}
}


